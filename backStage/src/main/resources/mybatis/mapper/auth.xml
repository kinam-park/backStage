<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xii.sql.auth">
    <resultMap id="resultUser" type="UserVO">
        <result property="userId" column="user_id" />
        <result property="roleId" column="role_id" />
        <result property="email" column="email" />
        <result property="password" column="password" />
        <result property="name" column="name" />
        <result property="company" column="company" />
        <result property="phone1" column="phone1" />
        <result property="phone2" column="phone2" />
        <result property="stopCondition" column="stop_condition" />
        <result property="notificationId" column="notification_id" />
        <result property="notificationEmail" column="notification_email" />
        <result property="lastLoginAt" column="last_login_at" />
        <result property="createdAt" column="created_at" />
        <result property="createdAtYmd" column="created_at_ymd" />
        <result property="timestampLastLogin" column="timestamp_last_login" />
        <result property="timestampCreated" column="timestamp_created" />
        <result property="roleName" column="role_name" />
        <result property="code" column="code" />
        <result property="availableService" column="available_service" />
        <result property="keywordLimit" column="keyword_limit" />
        <result property="expirationDate" column="expiration_date" />
    </resultMap>
    
    
    <select id="getTest" resultMap="resultUser">
	    SELECT 
			u.user_id
			,u.role_id
			,u.email
			,u.password
			,u.name
			,u.company
			,u.phone1
			,u.phone2
			,u.stop_condition
			,u.notification_id
			,u.notification_email
			,u.last_login_at
			,u.created_at
			,u.created_at_ymd
			,u.timestamp_last_login
			,u.timestamp_created
			,u.expiration_date
		FROM users u
		LIMIT 1
	</select>
    
    
</mapper>